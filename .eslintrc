{
	"extends": "airbnb/base",

	"env": {
		"es6": true,
		"node": true,
		"jquery": true,
		"browser": true
	},

	"plugins": ["react"],

	"ecmaFeatures": {
		"es6": true,
		"jsx": true
	},

	"globals": {
	},

	"rules": {
		"indent": [2, "tab"],
		"no-shadow": 1,
		"no-use-before-define": 0,
		"no-unused-vars": 1,
		"no-console": 0
	}
}
